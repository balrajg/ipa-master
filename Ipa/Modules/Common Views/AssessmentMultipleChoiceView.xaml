<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
xmlns="http://xamarin.com/schemas/2014/forms" 
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
xmlns:Triggers="clr-namespace:Ipa;assembly=Ipa" 
xmlns:local="clr-namespace:Ipa;assembly=Ipa" 
x:Class="Ipa.AssessmentMultipleChoiceView">
	<ContentView.Content>
		<StackLayout HorizontalOptions="FillAndExpand" Spacing="0" VerticalOptions="FillAndExpand" BackgroundColor="#FFFFFF">
			<StackLayout HorizontalOptions="FillAndExpand" Padding="10,10,10,65">
				<Label x:Name="Questions" HorizontalOptions="FillAndExpand" FontFamily="{x:Static local:Constants.FontStyle}" FontSize="14" TextColor="#2D2D2D" />
			</StackLayout>
			<StackLayout x:Name="OptionStackLayout" HorizontalOptions="FillAndExpand" Padding="10,10,10,10" Spacing="10">
			</StackLayout>
		</StackLayout>
	</ContentView.Content>
</ContentView>